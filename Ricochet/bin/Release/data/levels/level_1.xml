	<!-- switches 
	time lengh of the timer for timed switches

	SwitchType
	0 = toggle
	1 = timed
	2 = continuous
	3 = single

	switch_bits
	1 = toggle movement
	2 = toggle rotation
	4 = toggle enabled (will also toggle movement and rotation)
	to use a combination add the numbers eg. toggle movement and rotation then set to 3 (1+2)
	
	if disabled is set then activating the switch will enable the obstacle, if it...
	is not set then activating the switch will deactivate the obstacle

	disabled 		= is the object disable at load
	disabled_rot	= is the rotation disabled at load
	disabled_mov	= is the movement disabled at load
	1 = disabled
	0 = enabled

	-->
	<object type="switch" switch_type="1" time="3" position="5,-3">
		<object type="laser" position="-3.5,9.5" laser_direction="0,-1" p1="-3,0" p2="3,0" moving="1" duration="5" time="0" start_angle="0" delta_angle="45" rotation_duration="10" rotating="0" rotation_time="0" switch_bits="4" disabled="0" disabled_rot="0" disabled_mov="0"/>
