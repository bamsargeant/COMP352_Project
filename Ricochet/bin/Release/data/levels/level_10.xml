<?xml version="1.0"?>
<root>

	<!-- switches 
	time lengh of the timer for timed switches

	SwitchType
	0 = toggle
	1 = timed
	2 = continuous
	3 = single

	switch_bits
	1 = toggle movement
	2 = toggle rotation
	4 = toggle enabled (will also toggle movement and rotation)
	to use a combination add the numbers eg. toggle movement and rotation then set to 3 (1+2)
	
	if disabled is set then activating the switch will enable the obstacle, if it...
	is not set then activating the switch will deactivate the obstacle

	disabled 		= is the object disable at load
	disabled_rot	= is the rotation disabled at load
	disabled_mov	= is the movement disabled at load
	1 = disabled
	0 = enabled

	-->
	
	<!-- HOLE -->
	<object type="hole" position="10,0" />

	<!-- switches -->
	<object type="switch" switch_type="3" time="3" position="1.5,0">
		<object type="platform" position="4.5,0" size="0.5,5.4" switch_bits="4" disabled="0" />
	</object>

	<!-- LASERS -->
	
	<!-- PLAYER -->
	<object type="player" position="-10,0" />

	<!-- OUTER FRAME OF THE WALLS - starting top, working clockwise-->
	<object type="wall" position="0,4.5" size="24,1" rotation="0"/>
	<object type="wall" position="4.5, 3.35" size="1,1.3" rotation="0"/>
	<object type="wall" position="12.5, 0" size="1,8" rotation="0"/>	
	<object type="wall" position="0,-4.5" size="24,1" rotation="0"/>	
	<object type="wall" position="4.5, -3.35" size="1,1.3" rotation="0"/>
	<object type="wall" position="-12.5, 0" size="1,8" rotation="0"/>
	
	

	<!-- PLATFORMS -->
	

	

	
	
</root>